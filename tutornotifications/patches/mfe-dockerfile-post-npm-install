RUN --mount=type=cache,target=/root/.npm,sharing=shared echo "installing notifications plugin" && npm install @edx/frontend-plugin-notifications@^2.0.3 && echo "done"

RUN --mount=type=cache,target=/root/.npm,sharing=shared echo "TEMP installing custom header" && git clone --depth=1 --revision=refs/pull/638/head https://github.com/openedx/frontend-component-header.git && npm run build --prefix ./frontend-component-header && npm install ./frontend-component-header && echo "done"
